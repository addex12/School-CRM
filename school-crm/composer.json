{
    "name": "school-crm",
    "description": "A School CRM system built with Laravel, PHP, and JavaScript.",
    "type": "project",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.0",
        "laravel/sanctum": "^2.0",
        "laravel/jetstream": "^2.0",
        "pusher/pusher-php-server": "^7.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ],
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}